{
  "builds": [
    {
      "src": "api/**/*.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/onboard",
      "dest": "/api/onboard.py"
    },
    {
      "src": "/api/onboard/([^/]+)/status",
      "dest": "/api/onboard/[session_id]/status.py?session_id=$1"
    },
    {
      "src": "/api/create-agent",
      "dest": "/api/create-agent.py"
    },
    {
      "src": "/api/creation-status/([^/]+)",
      "dest": "/api/creation-status/[creation_id].py?creation_id=$1"
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ]
}